{
  "name": "windows-iana",
  "version": "4.2.1",
  "description": "A small tool to convert between Windows time zones and IANA",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubenillodo/windows-iana.git"
  },
  "author": "RubeÃÅn Illodo Brea <me@rubenillodo.com>",
  "license": "MIT",
  "scripts": {
    "precommit": "yarn lint && yarn test",
    "build-maps": "node util/build-zone-map && node util/build-iana-zones && node util/generate-enums",
    "build": "tsc -p tsconfig.json",
    "prettier": "prettier --write '**/*.{js,ts,json}'",
    "lint": "tslint -c tslint.json '{src,__tests__}/**/*.{js,ts}'",
    "test": "jest",
    "clean": "rimraf dist",
    "semantic-release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/mz": "^0.0.32",
    "@types/xml2js": "^0.4.4",
    "coveralls": "^3.0.6",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "mz": "^2.7.0",
    "pascal-case": "^2.0.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "semantic-release": "^15.13.21",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3",
    "xml2js": "^0.4.19"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      ".d.ts"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
